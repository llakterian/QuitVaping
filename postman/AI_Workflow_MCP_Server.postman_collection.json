    {
      "name": "AI Workflow Tests",
      "item": [
        {
          "name": "Test Complete Workflow - Struggling User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response contains motivation content\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('content');",
                  "    pm.expect(jsonData.content).to.have.property('type');",
                  "    pm.expect(jsonData.content.type).to.eql('mood_based');",
                  "});",
                  "",
                  "pm.test(\"Content is personalized for struggling mood\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.content.metadata.mood).to.eql('struggling');",
                  "    pm.expect(jsonData.content.content).to.include.oneOf(['struggle', 'tough', 'difficult', 'support']);",
                  "});",
                  "",
                  "pm.test(\"Learning update is provided\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('learning_update');",
                  "    pm.expect(jsonData.learning_update).to.have.property('personalized_data');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"method\": \"generate_motivation_content\",\n  \"params\": {\n    \"userId\": \"test_user_struggling\",\n    \"mood\": \"struggling\",\n    \"recentActivity\": [\n      {\n        \"activityType\": \"craving_logged\",\n        \"timestamp\": \"{{$isoTimestamp}}\",\n        \"data\": {\n          \"intensity\": 9,\n          \"trigger\": \"stress\",\n          \"resolved\": false\n        }\n      },\n      {\n        \"activityType\": \"craving_logged\",\n        \"timestamp\": \"2024-01-15T10:30:00Z\",\n        \"data\": {\n          \"intensity\": 8,\n          \"trigger\": \"anxiety\",\n          \"resolved\": false\n        }\n      }\n    ],\n    \"externalFactors\": {\n      \"weather\": \"rainy\",\n      \"timeOfDay\": \"evening\",\n      \"location\": \"home\",\n      \"additionalFactors\": {\n        \"dayOfWeek\": 1,\n        \"isWeekend\": false\n      }\n    }\n  }\n}"
            },
            "url": {
              "raw": "{{MCP_SERVER_URL}}/tools/generate_motivation_content",
              "host": ["{{MCP_SERVER_URL}}"],
              "path": ["tools", "generate_motivation_content"]
            }
          },
          "response": []
        },
        {
          "name": "Test Complete Workflow - Motivated User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response contains motivation content\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('content');",
                  "    pm.expect(jsonData.content).to.have.property('type');",
                  "    pm.expect(jsonData.content.type).to.eql('mood_based');",
                  "});",
                  "",
                  "pm.test(\"Content is personalized for motivated mood\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.content.metadata.mood).to.eql('motivated');",
                  "    pm.expect(jsonData.content.content).to.include.oneOf(['amazing', 'great', 'fantastic', 'energy']);",
                  "});",
                  "",
                  "pm.test(\"Relevance score is appropriate\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData.content.relevanceScore).to.be.above(0.5);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"method\": \"generate_motivation_content\",\n  \"params\": {\n    \"userId\": \"test_user_motivated\",\n    \"mood\": \"motivated\",\n    \"recentActivity\": [\n      {\n        \"activityType\": \"milestone_reached\",\n        \"timestamp\": \"{{$isoTimestamp}}\",\n        \"data\": {\n          \"milestone\": \"1_week\",\n          \"achievement\": \"1 week smoke-free\"\n        }\n      },\n      {\n        \"activityType\": \"exercise\",\n        \"timestamp\": \"2024-01-15T07:00:00Z\",\n        \"data\": {\n          \"type\": \"running\",\n          \"duration\": 30,\n          \"intensity\": \"high\"\n        }\n      }\n    ],\n    \"externalFactors\": {\n      \"weather\": \"sunny\",\n      \"timeOfDay\": \"morning\",\n      \"location\": \"park\",\n      \"additionalFactors\": {\n        \"dayOfWeek\": 6,\n        \"isWeekend\": true\n      }\n    }\n  }\n}"
            },
